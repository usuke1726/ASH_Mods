{
    "node.Avery.BeforeJA1.01": "よう",
    "node.Avery.BeforeJA1.02": "こんにちは　カーター！",
    "node.Avery.BeforeJA1.03": "何か　用かしら？",
    "node.Avery.BeforeJA1.04": "あー　その",
    "node.Avery.BeforeJA1.05": "今　黄金の羽根を　売ってるんだが",
    "node.Avery.BeforeJA1.06": "1枚　買わねぇか？",
    "node.Avery.BeforeJA1.07": "ごめんなさい",
    "node.Avery.BeforeJA1.08": "羽根なら　わたし　結構　持ってるのよ！",
    "node.Avery.BeforeJA1.09": "チッ　そうだったのか",
    "node.Avery.BeforeJA1.10": "他の人に　売ってあげて！",
    "node.Avery.BeforeJA1.11": "限定生産だし　誰かしら\n買ってくれると　思うわ",
    "node.Avery.BeforeJA1.12": "ああ　そうするよ",

    "node.Avery.BeforeJA2.01": "ねえねえ　今ヒマ？",
    "node.Avery.BeforeJA2.02": "ヒマじゃねぇよ",
    "node.Avery.BeforeJA2.03": "あら　残念",
    "node.Avery.BeforeJA2.04": "パルクールレースに誘おうと　思ってたのに！",
    "node.Avery.BeforeJA2.05": "パルクールレース？",
    "node.Avery.BeforeJA2.06": "お前　マラソン選手じゃ　なかったのか？",
    "node.Avery.BeforeJA2.07": "それがね！",
    "node.Avery.BeforeJA2.08": "今　アタシの中では\nパルクールが　大ブームなのよ！",
    "node.Avery.BeforeJA2.09": "この前レースして　すっごく楽しかった！",
    "node.Avery.BeforeJA2.10": "あなたとも　是非走りたいの！",
    "node.Avery.BeforeJA2.11": "オレは　やらねぇよ",
    "node.Avery.BeforeJA2.12": "あら　そう？",
    "node.Avery.BeforeJA2.13": "でも　気が変わるかも　しれないわ",
    "node.Avery.BeforeJA2.14": "競争したくなったら　また声かけてね！",
    "node.Avery.BeforeJA2.15": "へいへい",

    "node.Avery.BeforeJA3.01": "レースしたくなった！？",
    "node.Avery.BeforeJA3.02": "ならねぇよ",
    "node.Avery.BeforeJA3.03": "もう　イジワル！",

    "node.Avery.AfterJA1.01": "こんにちは！",
    "node.Avery.AfterJA1.02": "よう",
    "node.Avery.AfterJA1.03": "{{JATrigger}}から　あなたの話を聞いたわ",
    "node.Avery.AfterJA1.04": "そうかい",
    "node.Avery.AfterJA1.05": "色々　困っているようだけど…",
    "node.Avery.AfterJA1.06": "それなら　なおさら　わたしと\nパルクールレース　しましょう！！",
    "node.Avery.AfterJA1.07": "はぁ？",
    "node.Avery.AfterJA1.08": "なんでだよ？",
    "node.Avery.AfterJA1.09": "そりゃあ",
    "node.Avery.AfterJA1.10": "走ると　気分も　すっきりするし！",
    "node.Avery.AfterJA1.11": "それに　今だけ…",
    "node.Avery.AfterJA1.12": "わたしに勝ったら　コインを　差し上げます！",
    "node.Avery.AfterJA1.13": "ほう",
    "node.Avery.AfterJA1.14": "どう？やってみない？",
    "node.Avery.AfterJA1.H15": "…",
    "node.Avery.AfterJA1.H16": "やっぱ　いいわ",
    "node.Avery.AfterJA1.H17": "今は　そういう　気分じゃねぇ",
    "node.Avery.AfterJA1.H18": "そう　残念…",
    "node.Avery.AfterJA1.H19": "でも　いつかは\nそういう気分になるかも\nしれないのよね！？",
    "node.Avery.AfterJA1.H20": "ハイハイ　そん時が　きたらな",
    "node.Avery.AfterJA1.H21": "いつでも　待ってるわ！",

    "node.Avery.AfterJA2.01": "レースしたくなった？",
    "node.Avery.AfterJA2.H02": "いや　別に…",
    "node.Avery.AfterJA2.H03": "あら　残念",
    "node.Avery.AfterJA2.H04": "いつでも　待ってるわ！",

    "node.Avery.AfterJABody.O1": "いや　やめとくわ",
    "node.Avery.AfterJABody.O2": "受けて立とう",
    "node.Avery.AfterJABody.O1.01": "あら　残念",
    "node.Avery.AfterJABody.O1.02": "いつでも　待ってるわ！",
    "node.Avery.AfterJABody.01": "そう　こなくっちゃ！",
    "node.Avery.AfterJABody.02": "コースはここから　灯台の扉までよ！",
    "node.Avery.AfterJABody.03": "使える<color=red>黄金の羽根は　3つ</color>まで！",
    "node.Avery.AfterJABody.04": "準備はいい？",
    "node.Avery.AfterJABody.05": "ああ　いいぜ",
    "node.Avery.AfterJABody.06": "それじゃ　いくわよ…",
    "node.Avery.AfterJABody.NotEquippedYet.01": "と　いきたいところ　だけど",
    "node.Avery.AfterJABody.NotEquippedYet.02": "あなた　ランニングシューズを\nはいていないじゃない！",
    "node.Avery.AfterJABody.NotHasShoes.01": "おっと　そうだった" ,
    "node.Avery.AfterJABody.NotHasShoes.02": "シューズを手に入れてから　レースしましょ！",
    "node.Avery.AfterJABody.NotHasShoes.03": "ああ　分かった",
    "node.Avery.AfterJABody.HasShoes.01": "おっと　そうだった",
    "node.Avery.AfterJABody.HasShoes.02": "一応　持ってはいるが",
    "node.Avery.AfterJABody.HasShoes.03": "後で売るつもり　だったんだよな…",
    "node.Avery.AfterJABody.HasShoes.04": "いいじゃない　はいちゃいましょ！",
    "node.Avery.AfterJABody.HasShoes.05": "うーん",
    "node.Avery.AfterJABody.HasShoes.06": "そうするか",
    "node.Avery.AfterJABody.HasShoes.07": "よし　これで　準備万端ね！",
    "node.Avery.AfterJABody.HasShoes.08": "それじゃ　いくわよ…",

    "node.Avery.FirstRaceWin.01": "ハァ　ハァ",
    "node.Avery.FirstRaceWin.02": "あなた",
    "node.Avery.FirstRaceWin.03": "すごく　速いのね！",
    "node.Avery.FirstRaceWin.04": "びっくりした！",
    "node.Avery.FirstRaceWin.05": "どうも",
    "node.Avery.FirstRaceWin.06": "オレも　まさか　勝つとは　思ってなかった",
    "node.Avery.FirstRaceWin.07": "しかし",
    "node.Avery.FirstRaceWin.08": "結構　楽しいもんだな",
    "node.Avery.FirstRaceWin.09": "それは　良かった！",
    "node.Avery.FirstRaceWin.10": "また　いつでも　再戦を受け付けるわ",
    "node.Avery.FirstRaceWin.11": "今後は勝つから！",
    "node.Avery.FirstRaceWin.12": "あっ　そうだ！",
    "node.Avery.FirstRaceWin.13": "次　レースするときのために",
    "node.Avery.FirstRaceWin.14": "これを　渡しておくわね！",
    "node.Avery.FirstRaceWin.15": "いつでも　挑戦を　受け付けるわ！",
    "node.Avery.FirstRaceWin.16": "おう",

    "node.Avery.FirstRaceLose.01": "やった　勝ったわ！",
    "node.Avery.FirstRaceLose.02": "ああ　クソッ！",
    "node.Avery.FirstRaceLose.03": "次は　絶対　勝つ",
    "node.Avery.FirstRaceLose.04": "望むところよ！",
    "node.Avery.FirstRaceLose.05": "それで",
    "node.Avery.FirstRaceLose.06": "？",
    "node.Avery.FirstRaceLose.07": "パルクール　結構　楽しい　でしょう？",
    "node.Avery.FirstRaceLose.08": "まあ　思った以上　かな",
    "node.Avery.FirstRaceLose.09": "ふふっ",
    "node.Avery.FirstRaceLose.10": "また　いつでも　再戦を受け付けるわ",
    "node.Avery.FirstRaceLose.11": "次も　負けないわよ！",
    "node.Avery.FirstRaceLose.12": "おう　次は負かしてやるよ",
    "node.Avery.FirstRaceLose.13": "あっ　そうだ！",
    "node.Avery.FirstRaceLose.14": "次　レースするときのために",
    "node.Avery.FirstRaceLose.15": "これを　渡しておくわね！",
    "node.Avery.FirstRaceLose.16": "いつでも　挑戦を　受け付けるわ！",
    "node.Avery.FirstRaceLose.17": "おう",

    "node.Avery.RaceStart.01": "あら　カーター！",
    "node.Avery.RaceStart.02": "もう一度　レースする？",
